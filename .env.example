RESEND_API_KEY=your_resend_api_key_here

# Optional: Resend API (alternative to SendGrid)
# Get from https://resend.com
# If both RESEND_API_KEY and SENDGRID_API_KEY are set, Resend will be used
RESEND_API_KEY=your_resend_api_key_here

# Rural Area Batch Processing
