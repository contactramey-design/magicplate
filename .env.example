# REQUIRED - Get from sendgrid.com (free tier = 100 emails/day)
SENDGRID_API_KEY=your_sendgrid_api_key_here

# REQUIRED - At least one API key needed
GOOGLE_PLACES_API_KEY=your_google_places_api_key_here
# OR
YELP_API_KEY=your_yelp_api_key_here

# Your search area
SEARCH_AREA=Los Angeles, CA

# Email settings
FROM_EMAIL=sydney@magicplate.info
FROM_NAME=Sydney Ramey - MagicPlate.ai

# Optional: n8n webhook for automation
N8N_WEBHOOK_URL=

# Qualification threshold (minimum score to qualify)
QUALIFICATION_THRESHOLD=40

# Optional: Outscraper API (alternative scraping source)
# Get from https://outscraper.com
OUTSCRAPER_API_KEY=your_outscraper_api_key_here

# Optional: Resend API (alternative to SendGrid)
# Get from https://resend.com
