{
  "name": "magicplate-outreach",
  "version": "1.0.0",
  "description": "Lead scraping and outreach system for MagicPlate.ai",
  "main": "index.js",
  "scripts": {
    "dev": "node server.js",
    "dev:static": "npx serve public -l 3000",
    "scrape": "node scripts/scrape-restaurants.js",
    "scrape:rural": "node scripts/scrape-rural-batch.js",
    "email": "node scripts/send-email.js",
    "email:test": "node scripts/send-email.js --max=5",
    "email:preview": "node scripts/send-email.js --dry-run",
    "outreach": "node scripts/multi-channel-outreach.js",
    "outreach:test": "node scripts/multi-channel-outreach.js --max=5",
    "outreach:preview": "node scripts/multi-channel-outreach.js --dry-run",
    "outreach:email-only": "node scripts/multi-channel-outreach.js --channels=email",
    "outreach:whatsapp-only": "node scripts/multi-channel-outreach.js --channels=whatsapp",
    "track": "node scripts/track-responses.js",
    "followups": "node scripts/run-followups.js",
    "requalify": "node scripts/re-qualify-leads.js",
    "clean": "node scripts/clean-leads.js",
    "start": "node scripts/scrape-restaurants.js && node scripts/send-email.js",
    "openclaw:install": "curl -fsSL https://openclaw.ai/install.sh | bash || npm i -g openclaw",
    "openclaw:onboard": "openclaw onboard",
    "openclaw:daily": "node openclaw-skills/index.js dailyAutomation",
    "openclaw:scrape": "node -e \"require('./openclaw-skills').scrapeRestaurants(process.env.SEARCH_AREA || 'Los Angeles, CA', 100)\"",
    "openclaw:email": "node -e \"require('./openclaw-skills').sendEmails(20)\"",
    "openclaw:outreach": "node -e \"require('./openclaw-skills').sendOutreach(10, ['email', 'whatsapp'])\""
  },
  "dependencies": {
    "@sendgrid/mail": "^8.1.0",
    "axios": "^1.7.9",
    "cheerio": "^1.0.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "form-data": "^4.0.1",
    "resend": "^4.0.0",
    "pg": "^8.13.1",
    "stripe": "^17.3.1",
    "jsonwebtoken": "^9.0.2",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1"
  },
  "overrides": {
    "glob": "^11.0.0",
    "rimraf": "^6.0.0",
    "tar": "^7.4.0",
    "inflight": "npm:@isaacs/inflight@^1.0.6"
  }
}
